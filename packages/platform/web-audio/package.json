{
  "name": "@soundblue/web-audio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Web Audio API adapters and instruments with dual implementation for SSG safety",
  "exports": {
    ".": {
      "types": "./src/types.ts",
      "browser": "./src/index.browser.ts",
      "default": "./src/index.noop.ts"
    },
    "./context": {
      "types": "./src/types.ts",
      "browser": "./src/context/index.browser.ts",
      "default": "./src/context/index.noop.ts"
    },
    "./instruments": {
      "types": "./src/instruments/index.browser.ts",
      "browser": "./src/instruments/index.browser.ts",
      "default": "./src/instruments/index.noop.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    }
  },
  "main": "./src/index.browser.ts",
  "types": "./src/types.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "tone": "^15.1.22",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
