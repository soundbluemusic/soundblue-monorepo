{
  "name": "@soundblue/pwa",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "description": "PWA utilities - config presets, hooks, and SW registration",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "browser": "./src/index.browser.ts",
      "default": "./src/index.noop.ts"
    },
