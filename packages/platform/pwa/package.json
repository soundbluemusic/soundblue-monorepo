      "types": "./src/hooks/index.ts",
      "browser": "./src/hooks/index.ts",
      "default": "./src/hooks/index.noop.ts"
    }
  },
  "main": "./src/index.browser.ts",
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.10",
    "react": "^19.2.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
