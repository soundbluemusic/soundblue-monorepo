{
  "name": "@soundblue/shared-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Shared utilities and types for SoundBlue React apps",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "import": "./src/hooks/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts"
    },
    "./providers": {
      "types": "./src/providers/index.ts",
      "import": "./src/providers/index.ts"
    },
    "./storage": {
      "types": "./src/storage/index.ts",
      "import": "./src/storage/index.ts"
    },
    "./styles": "./src/styles/index.scss",
    "./styles/*": "./src/styles/*"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "dependencies": {
    "clsx": "^2.1.1",
    "dexie": "^4.2.1"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.1",
    "zustand": "^5.0.6"
  },
  "peerDependenciesMeta": {
    "react-router": {
      "optional": true
    },
    "zustand": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "zustand": "^5.0.6"
  }
}
